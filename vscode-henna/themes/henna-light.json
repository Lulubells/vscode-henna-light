  "tokenColors": [
    {
      "name": "Iron",
      "scope": ["support.type.property-name", "support.variable", "variable"],
      "settings": {
        "foreground": "#324357"
      }
    },
    {
      "name": "Cornflower Blue",
      "scope": [
        "markup.inline.raw",
        "storage.type",
        "support.constant",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "Fountain Blue",
      "scope": ["constant", "keyword.other.unit"],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Shuttle Gray",
      "scope": ["comment"],
      "settings": {
        "foreground": "#606F73"
      }
    },
    {
      "name": "Raven",
      "scope": [
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.string",
        "punctuation.definition.tag",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "Olivine",
      "scope": ["string", "markup.quote", "markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#1abc9c"
      }
    },
    {
      "name": "Whiskey",
      "scope": [
        "entity.name.class",
        "entity.name.function",
        "entity.name.type",
        "entity.other.attribute-name",
        "entity.other.inherited-class",
        "markup.heading.setext",
        "support.function"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Harvest Gold",
      "scope": ["entity.name.tag", "storage", "variable.language"],
      "settings": {
        "foreground": "#9cd230"
      }
    },
    {
      "name": "Sunglo",
      "scope": ["keyword", "punctuation.definition.keyword"],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Bold",
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
